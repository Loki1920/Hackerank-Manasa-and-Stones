#User function Template for python3

from typing import List
from queue import Queue
class Solution:
    #Function to return Breadth First Traversal of given graph.
    def bfsOfGraph(self, V: int, adj: List[List[int]]) -> List[int]:
        v = []
        q = []
        
        q.append(0)
        v.append(0)
        while q:
            s = q.pop(0)
            for i in adj[s]:
                if i not in v:
                    v.append(i)
                    q.append(i)
        return v
            
        
        
        # code here


